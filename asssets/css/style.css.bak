/****************************
       1.Header Area Css 
*****************************/
.header-area{
    padding: 30px 0;
    padding: 3rem 0;
    background-color: #fff;
    z-index: 999;
}
.active-bg{
    background-color: #fff;
    box-shadow: 0px 0px 10px rgba(0,0,0,0.15) !important;
    width: 100%;
    position: fixed;
    padding: 20px 0;
    padding: 2rem 0;
    -webkit-transition: .5s;
    transition: .5s;
}
.header-area .fixed{
    opacity: 0;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    -webkit-transform: translateY(-50%);
            transform: translateY(-50%);
}
.menu ul {
	list-style: none;
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 0;
	-webkit-box-pack: end;
	    -ms-flex-pack: end;
	        justify-content: flex-end;
	margin: 0;
}
.menu ul li{
    margin: 0 20px;
    margin: 0 2rem;
}
.header-area .menu ul li .nav-link{
    color: #2a2a2a !important;
    font-size: 15px;
    font-size: 1.5rem;
    font-weight: 500;
    text-decoration: none;
    -webkit-transition: .5s;
    transition: .5s;
    display: inline-block;
    padding: 0 !important;
    background-color: none !important;
    border-radius: inherit;
}
.nav-pills .nav-link.active,
.nav-pills .show > .nav-link {
	background-color: transparent !important;
}
.menu ul li .nav-link:hover{
    color: #ff685f !important;
}
.menu ul li .nav-link.active{
    color: #ff685f !important;
}
.header-top-btn{
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: end;
        -ms-flex-pack: end;
            justify-content: flex-end;
    -webkit-box-align: center;
        -ms-flex-align: center;
            align-items: center;
}
.header-btn{
    text-decoration: none;
    color: #fff;
    font-size: 15px;
    font-size: 1.5rem;
    background-color: #ff685f;
    display: inline-block;
    padding: 8px 20px;
    padding: .8rem 2rem;
    border-radius: 23px;
    -webkit-transition: .5s;
    transition: .5s;
}
.header-btn:hover{
    background-color: #03a4ed ;
    color: #fff;
}
.toggle-icon {
	text-decoration: none;
	font-size: 20px;
	font-weight: 500;
	color: #2a2a2a !important;
	border: 1px solid #2a2a2a;
	padding: 4px 12px;
