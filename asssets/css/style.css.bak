	border-radius: 6px;
    display: none;
}
.toggle-icon:hover{
    background-color: #03a4ed;
    border-color: transparent;
    color: #fff !important;
}
/****************************
       1.Bannar Area Css 
*****************************/
.bannar-area{
    position: relative;
    padding: 180px 0 100px 0;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;
}
.bannar-area:before{
    content: "";
