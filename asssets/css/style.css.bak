    background-repeat: no-repeat;
    background-size: cover;
    position: absolute;
    right: 0;
    top: 15px;
    width: 819px;
    height: 674px;
    z-index: -1;
}
.slider-text {
	margin-right: 30px;
}
.slider-text h4 {
	font-size: 20px;
	font-weight: 500;
	color: #ff685f;
	margin-bottom: 10px;
	margin-bottom: 1rem;
}
.slider-text h1 {
	line-height: 70px;
	margin-bottom: 20px;
	margin-bottom: 2rem;
}
.slider-text p{
    margin: 20px 0;
    margin: 2rem 0;
}
.inner-btn-down{
    display: -webkit-inline-box;
    display: -ms-inline-flexbox;
    display: inline-flex;
}
.vedio-btn a {
	display: inline-block;
	text-decoration: none;
	font-size: 17px;
	color: #fff;
	background: #03a4ed;
	padding: 10px 20px;
	border-radius: 23px;
}
.call-btn a {
	text-decoration: none;
	display: inline-block;
	font-size: 17px;
	margin-left: 10px;
}
.call-btn a i{
    margin-right: 10px;
	color: #fff;
	background: #ff685f;
	height: 45px;
    width: 45px;
	border-radius: 50%;
    line-height: 45px;
    text-align: center;
}
.bannar-area .slider-items .owl-dots {
    counter-reset: dots;
    margin-top: 40px !important;
    margin-top: 4rem !important;
    text-align: left;
}
.bannar-area .slider-items .owl-dot:before {
	counter-increment: dots;
	content: counter(dots);
	font-size: 20px;
	font-weight: 500;
	margin-left: 10px;
	color: #2a2a2a;
	width: 15px;
	display: inline-block;
	text-align: center;
	border-bottom: 3px solid transparent;
	-webkit-transition: all .5s;
	transition: all .5s;
    margin-top: 30px;
    margin-top: 3rem;
}
.bannar-area .slider-items .active:before {
	color: #ff685f;
	border-bottom: 3px solid #ff685f;
}
.bannar-area  .owl-theme .owl-dots .owl-dot span {
	background: none !important;
}
.bannar-area  .owl-theme .owl-dots .owl-dot.active span, 
.bannar-area  .owl-theme .owl-dots .owl-dot:hover span {
	background: none !important;
}
